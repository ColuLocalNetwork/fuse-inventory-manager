{
  "/currency": {
    "POST": {
      "route": "create",
      "jwt": true,
      "validators": {
        "body": {
          "address": {
            "notEmpty": true,
            "errorMessage": "is missing"
          },
          "creationTransactionHash": {
            "notEmpty": true,
            "errorMessage": "is missing"
          },
          "creationBlockHash": {
            "notEmpty": true,
            "errorMessage": "is missing"
          },
          "creationBlockNumber": {
            "notEmpty": true,
            "errorMessage": "is missing"
          },
          "externalId": {
            "optional": true
          }
        }
      }
    }
  },
  "/currency/id/:id": {
    "PUT": {
      "route": "edit",
      "jwt": true,
      "validators": {
        "params": {
          "id": {
            "notEmpty": true,
            "errorMessage": "is missing"
          }
        }
      }
    },
    "GET": {
      "route": "get",
      "jwt": true,
      "validators": {
        "params": {
          "id": {
            "notEmpty": true,
            "errorMessage": "is missing"
          }
        }
      }
    }
  },
  "/currency/address/:address": {
    "PUT": {
      "route": "editByAddress",
      "jwt": true,
      "validators": {
        "params": {
          "address": {
            "notEmpty": true,
            "errorMessage": "is missing"
          }
        }
      }
    },
    "GET": {
      "route": "getByAddress",
      "jwt": true,
      "validators": {
        "params": {
          "address": {
            "notEmpty": true,
            "errorMessage": "is missing"
          }
        }
      }
    }
  }
}